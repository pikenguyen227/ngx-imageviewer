{
  "name": "ngx-imageviewer-app",
  "version": "0.0.0-development",
  "author": "Hallyson Almeida <hallysonh@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emazv72/ngx-imageviewer"
  },
  "homepage": "https://github.com/emazv72/ngx-imageviewer#readme",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run build:lib",
    "build:lib": "ng build ngx-imageviewer --configuration production && cp README.md dist/ngx-imageviewer",
    "build:docs": "ng build ngx-imageviewer-app --configuration production",
    "postbuild": "tar -C dist --overwrite -czf dist/ngx-imageviewer.tgz ngx-imageviewer",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/platform-server": "^12.2.0",
    "@angular/router": "^12.2.0",
    "rxjs": "^6.6.3",
    "showdown": "^1.9.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.0",
    "@angular/cli": "^12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@types/jasmine": "^3.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^12.11.1",
    "@types/showdown": "^1.7.3",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.1",
    "cp": "^0.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.0",
    "karma": "^6.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "protractor": "^7.0.0",
    "puppeteer": "^13.0.0",
    "semantic-release": "^18.0.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.0",
    "typescript": "^4.2.0"
  },
  "release": {
    "pkgRoot": "dist/ngx-imageviewer",
    "assets": [
      "dist/ngx-imageviewer.tgz"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  }
}
